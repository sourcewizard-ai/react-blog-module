{
  "name": "react-blog-module",
  "version": "0.1.5",
  "description": "Embeddable markdown blog page",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./posts": {
      "types": "./dist/posts.d.ts",
      "import": "./dist/posts.js"
    },
    "./mdx": {
      "types": "./dist/mdx.d.ts",
      "import": "./dist/mdx.js"
    },
    "./util": {
      "types": "./dist/util.d.ts",
      "import": "./dist/util.js"
    },
    "./author-card": {
      "types": "./dist/author-card.d.ts",
      "import": "./dist/author-card.js"
    },
    "./posts-page": {
      "types": "./dist/posts-page.d.ts",
      "import": "./dist/posts-page.js"
    },
    "./singe-post-page": {
      "types": "./dist/singe-post-page.d.ts",
      "import": "./dist/singe-post-page.js"
    },
    "./mdx/components": {
      "types": "./dist/mdx/components.d.ts",
      "import": "./dist/mdx/components.js"
    },
    "./mdx/code-block": {
      "types": "./dist/mdx/code-block.d.ts",
      "import": "./dist/mdx/code-block.js"
    },
    "./mdx/task": {
      "types": "./dist/mdx/task.d.ts",
      "import": "./dist/mdx/task.js"
    }
  },
  "files": [
    "dist",
    "*.ts",
    "*.tsx",
    "mdx/",
    "README.md"
  ],
  "peerDependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.456.0",
    "next-mdx-remote": "^5.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^9.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "keywords": [
    "blog",
    "mdx",
    "react",
    "typescript"
  ],
  "author": "Ivan Chebykin",
  "license": "MIT",
  "readme": "README.md",
  "homepage": "https://github.com/sourcewizard-ai/react-blog-module",
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcewizard-ai/react-blog-module.git"
  },
  "bugs": {
    "url": "https://github.com/sourcewizard-ai/react-blog-module/issues"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0"
  }
}
